{
	"extends": "tsconfig/base.json",
	"include": ["**/*.ts", "**/*.tsx", "src/**/*.ts", "src/**/*.tsx"],
	"exclude": ["dist", "build", "node_modules"],
	"compilerOptions": {
		"baseUrl": ".",
		"paths": {
			"@/*": ["./src/*"],
			"@config": ["../../config"],
			"@proxed/structure": ["../../packages/structure/src"],
			"@proxed/utils": ["../../packages/utils/src"],
			"@proxed/jobs": ["../../packages/jobs/src"],
			"@proxed/kv": ["../../packages/kv/src"]
		},
		"strict": true
	}
}
