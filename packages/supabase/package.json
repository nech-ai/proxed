{
	"name": "@proxed/supabase",
	"version": "1.0.0",
	"private": true,
	"scripts": {
		"clean": "rm -rf .turbo node_modules",
		"dev": "supabase start",
		"login": "supabase login",
		"migrate": "supabase migration up",
		"migration:create": "supabase migration new",
		"seed": "supabase db seed generate && supabase db seed run",
		"reset": "supabase db reset",
		"generate": "supabase gen types --lang=typescript --local --schema public > src/types/db.generated.ts",
		"generate-functions": "supabase gen types --lang=typescript --local --schema functions > src/types/functions.generated.ts",
		"functions": "supabase functions serve",
		"lint": "biome check .",
		"format": "biome format --write .",
		"typecheck": "tsc --noEmit"
	},
	"dependencies": {
		"@date-fns/utc": "^2.1.1",
		"@proxed/logger": "workspace:*",
		"@supabase/ssr": "^0.8.0",
		"date-fns": "^4.1.0",
		"react": "19.2.3",
		"server-only": "^0.0.1",
		"supabase": "^2.70.5"
	},
	"devDependencies": {
		"@supabase/supabase-js": "^2.89.0"
	},
	"exports": {
		"./server": "./src/clients/server.ts",
		"./client": "./src/clients/client.ts",
		"./api": "./src/clients/api.ts",
		"./job": "./src/clients/job.ts",
		"./mutations": "./src/mutations/index.ts",
		"./middleware": "./src/clients/middleware.ts",
		"./queries": "./src/queries/index.ts",
		"./types": "./src/types/index.ts",
		"./cached-queries": "./src/queries/cached-queries.ts",
		"./storage": "./src/utils/storage.ts"
	}
}
